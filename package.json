    "@shopify/shopify-app-session-storage": "^3.0.6",
    "@shopify/shopify-app-session-storage-memory": "^4.0.6",
    "@shopify/shopify-app-session-storage-mongodb": "^4.0.6",
    "@shopify/shopify-app-session-storage-sqlite": "^4.0.6",
    "@types/mongoose": "^5.11.97",
    "axios": "^1.7.7",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "gql": "^1.1.2",
    "graphql": "^16.9.0",
    "graphql-request": "^7.1.0",
    "mongodb": "^6.9.0",
    "mongoose": "^8.7.0",
    "openai": "^4.67.3",
    "pdf-lib": "^1.17.1",
    "pdf-parse": "^1.1.1",
    "pdf-ts": "^0.0.2",
    "pdfjs-dist": "^4.8.69",
    "pinecone-client": "^2.0.0",
    "semver": "^7.6.2",
    "socket.io": "^4.4.1",
    "socket.io-client": "^4.8.0",
    "socket.io-file": "^2.0.31",
    "sqlite3": "^5.1.7",
    "uuid": "^11.0.3"
