{
  "name": "my-shopify-app",
  "version": "1.0.0",
  "description": "A Shopify app built with Express and TypeScript",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "start": "node dist/index.js",
    "superstart": "concurrently \"npm run server\" \"npm run client\"",
    "dev": "NODE_OPTIONS='--loader ts-node/esm' ts-node src/index.ts",
    "build": "tsc",
    "lint": "eslint . --ext .ts",
    "client": "node dist/clientTest.js"
  },
  "dependencies": {
    "@shopify/shopify-api": "^11.4.1",
    "@shopify/shopify-app-express": "^5.0.6",
    "@shopify/shopify-app-session-storage": "^3.0.6",
    "@shopify/shopify-app-session-storage-memory": "^4.0.6",
    "@vitejs/plugin-react": "^4.3.2",
    "axios": "^1.7.7",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "semver": "^7.6.2",
    "socket.io": "^4.8.0",
    "socket.io-client": "^4.8.0",
    "tsx": "^4.19.1",
    "vite": "^5.4.8"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/react": "^18.3.10",
    "@types/react-dom": "^18.3.0",
    "@types/semver": "^7.5.8",
    "@types/socket.io-client": "^3.0.0",
    "concurrently": "^9.0.1",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "jsonwebtoken": "^9.0.2",
    "nodemon": "^3.1.7",
    "rimraf": "^5.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2"
  },
  "keywords": [
    "shopify",
    "express",
    "typescript",
    "app"
  ],
  "author": "suryakangeyan",
  "license": "MIT"
}
