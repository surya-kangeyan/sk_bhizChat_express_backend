{
  "name": "my-shopify-app",
  "version": "1.0.0",
  "description": "A Shopify app built with Express and TypeScript",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "NODE_OPTIONS='--loader ts-node/esm' ts-node -r tsconfig-paths/register src/index.ts",
    "postinstall": "npm run build",
    "build": "tsc",
    "lint": "eslint . --ext .ts",
