  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22.7.7",
