    "@types/socket.io-file": "^2.0.7",
    "@types/sqlite3": "^3.1.11",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "jsonwebtoken": "^9.0.2",
    "nodemon": "^3.0.1",
    "rimraf": "^5.0.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.6.2"
